<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primary_grey">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="1270dp"
        android:layout_height="183dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.004">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="1279dp"
            android:layout_height="203dp"
            android:background="@color/primary_white"
            android:minHeight="?attr/actionBarSize"
            android:theme="?attr/actionBarTheme"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <ImageView
            android:id="@+id/imageView2"
            android:contentDescription="@string/drink_espresso"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/textView5"
            app:layout_constraintHorizontal_bias="0.88"
            app:layout_constraintStart_toEndOf="@+id/buttonBack"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.494"
            app:srcCompat="@drawable/espresso_icon" />

        <ImageButton
            android:id="@+id/offButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="?android:selectableItemBackground"
            android:contentDescription="@string/button_off"
            app:layout_constraintBottom_toBottomOf="@+id/buttonBack"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.97"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/buttonBack"
            app:srcCompat="@drawable/off_button_icon" />

        <ImageView
            android:id="@+id/buttonBack"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:contentDescription="@string/back_button"
            app:layout_constraintBottom_toBottomOf="@+id/toolbar"
            app:layout_constraintEnd_toEndOf="@+id/toolbar"
            app:layout_constraintHorizontal_bias="0.027"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.491"
            app:srcCompat="@drawable/back_icon" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-medium"
            android:text="@string/drink_espresso"
            android:textColor="@color/black"
            android:textSize="60sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/toolbar"
            app:layout_constraintHorizontal_bias="0.549"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout8"
        android:layout_width="978dp"
        android:layout_height="437dp"
        android:background="@drawable/rounded_outline"
        android:backgroundTint="@color/primary_white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
        app:layout_constraintVertical_bias="0.271">

        <RadioGroup
            android:id="@+id/MilkRadioGroup"
            android:layout_width="442dp"
            android:layout_height="96dp"
            android:layout_marginTop="144dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="@+id/TempRadioGroup"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/TempRadioGroup"
            app:layout_constraintTop_toTopOf="parent">

            <RadioButton
                android:id="@+id/buttonfrothed"
                android:layout_width="wrap_content"
                android:layout_height="84dp"
                android:layout_marginStart="35dp"
                android:background="@drawable/radio_button_background"
                android:text="@string/attr_frothed"
                android:textAllCaps="false"
                android:textSize="25sp" />

            <RadioButton
                android:id="@+id/buttonlatte"
                android:layout_width="137dp"
                android:layout_height="84dp"
                android:layout_marginStart="60dp"
                android:background="@drawable/radio_button_background"
                android:checked="false"
                android:text="@string/attr_latte"
                android:textAllCaps="false"
                android:textSize="25sp" />
        </RadioGroup>

        <RadioGroup
            android:id="@+id/TempRadioGroup"
            android:layout_width="442dp"
            android:layout_height="93dp"
            android:layout_marginTop="24dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <RadioButton
                android:id="@+id/buttoncold"
                android:layout_width="wrap_content"
                android:layout_height="84dp"
                android:layout_marginStart="60dp"
                android:background="@drawable/radio_button_background"
                android:checked="false"
                android:text="@string/attr_cold"
                android:textAllCaps="false"
                android:textSize="25sp" />

            <RadioButton
                android:id="@+id/hotbutton"
                android:layout_width="137dp"
                android:layout_height="84dp"
                android:layout_marginStart="60dp"
                android:background="@drawable/radio_button_background"
                android:text="@string/attr_hot"
                android:textAllCaps="false"
                android:textSize="25sp" />
        </RadioGroup>

        <RadioGroup
            android:layout_width="643dp"
            android:layout_height="68dp"
            android:layout_marginTop="344dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <RadioButton
                android:id="@+id/amount1"
                android:layout_width="105dp"
                android:layout_height="60dp"
                android:layout_marginStart="50dp"
                android:background="@drawable/radio_button_background"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:text="@string/amount_small" />

            <RadioButton
                android:id="@+id/amount2"
                android:layout_width="105dp"
                android:layout_height="60dp"
                android:layout_marginStart="50dp"
                android:background="@drawable/radio_button_background"
                android:text="@string/amount_olga"
                android:textAllCaps="false"
                android:textSize="25sp" />

            <RadioButton
                android:id="@+id/amount3"
                android:layout_width="105dp"
                android:layout_height="60dp"
                android:layout_marginStart="50dp"
                android:background="@drawable/radio_button_background"
                android:text="@string/amount_big"
                android:textAllCaps="false"
                android:textSize="25sp" />

            <RadioButton
                android:id="@+id/amount4"
                android:layout_width="105dp"
                android:layout_height="60dp"
                android:layout_marginStart="50dp"
                android:background="@drawable/radio_button_background"
                android:text="@string/amount_gkiwnhs"
                android:textAllCaps="false"
                android:textSize="25sp" />
        </RadioGroup>

        <Button
            android:id="@+id/minusbuttoncups"
            android:layout_width="61dp"
            android:layout_height="67dp"
            android:background="@drawable/rounded_outline"
            android:text="@string/minus_sign"
            android:textSize="30sp"
            app:backgroundTint="@color/primary_grey"
            app:layout_constraintBottom_toBottomOf="@+id/cupsAmount"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.327"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/cupsAmount"
            app:layout_constraintVertical_bias="0.101" />

        <Button
            android:id="@+id/plusbuttoncups"
            android:layout_width="61dp"
            android:layout_height="67dp"
            android:background="@drawable/rounded_outline"
            android:text="+"
            android:textSize="30sp"
            app:backgroundTint="@color/primary_grey"
            app:layout_constraintBottom_toBottomOf="@+id/minusbuttoncups"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.42"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/minusbuttoncups"
            app:layout_constraintVertical_bias="0.101" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-medium"
            android:text="@string/attr_cups"
            android:textColor="@color/black"
            android:textSize="40sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.021"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.041" />

        <TextView
            android:id="@+id/cupsAmount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-medium"
            android:text="@string/default_attr_value"
            android:textColor="@color/black"
            android:textSize="50sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView20"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.179"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView20"
            app:layout_constraintVertical_bias="0.119" />

        <Button
            android:id="@+id/minusbuttonmilk"
            android:layout_width="61dp"
            android:layout_height="67dp"
            android:background="@drawable/rounded_outline"
            android:text="@string/minus_sign"
            android:textSize="30sp"
            app:backgroundTint="@color/primary_grey"
            app:layout_constraintBottom_toBottomOf="@+id/milkAmount"
            app:layout_constraintEnd_toEndOf="@+id/minusbuttoncups"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/minusbuttoncups"
            app:layout_constraintTop_toTopOf="@+id/milkAmount"
            app:layout_constraintVertical_bias="0.166" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-medium"
            android:text="@string/attr_milk"
            android:textColor="@color/black"
            android:textSize="40sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/textView20"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/textView20"
            app:layout_constraintTop_toBottomOf="@+id/textView20"
            app:layout_constraintVertical_bias="0.21" />

        <TextView
            android:id="@+id/milkAmount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-medium"
            android:text="@string/default_attr_value"
            android:textColor="@color/black"
            android:textSize="50sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView21"
            app:layout_constraintEnd_toEndOf="@+id/cupsAmount"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/cupsAmount"
            app:layout_constraintTop_toTopOf="@+id/textView21"
            app:layout_constraintVertical_bias="0.151" />

        <Button
            android:id="@+id/plusbuttonmilk"
            android:layout_width="61dp"
            android:layout_height="67dp"
            android:background="@drawable/rounded_outline"
            android:text="+"
            android:textSize="30sp"
            app:backgroundTint="@color/primary_grey"
            app:layout_constraintBottom_toBottomOf="@+id/minusbuttonmilk"
            app:layout_constraintEnd_toEndOf="@+id/plusbuttoncups"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/plusbuttoncups"
            app:layout_constraintTop_toTopOf="@+id/minusbuttonmilk"
            app:layout_constraintVertical_bias="0.166" />

        <TextView
            android:id="@+id/textView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-medium"
            android:text="@string/attr_sugar"
            android:textColor="@color/black"
            android:textSize="40sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/textView21"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/textView21"
            app:layout_constraintTop_toBottomOf="@+id/textView21"
            app:layout_constraintVertical_bias="0.32" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-medium"
            android:text="@string/attr_coffee"
            android:textColor="@color/black"
            android:textSize="40sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/textView22"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/textView22"
            app:layout_constraintTop_toBottomOf="@+id/textView22"
            app:layout_constraintVertical_bias="0.794" />

        <TextView
            android:id="@+id/sugarAmount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-medium"
            android:text="@string/default_attr_value"
            android:textColor="@color/black"
            android:textSize="50sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView22"
            app:layout_constraintEnd_toEndOf="@+id/milkAmount"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/milkAmount"
            app:layout_constraintTop_toTopOf="@+id/textView22"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/minusbuttonsugar"
            android:layout_width="61dp"
            android:layout_height="67dp"
            android:background="@drawable/rounded_outline"
            android:text="@string/minus_sign"
            android:textSize="30sp"
            app:backgroundTint="@color/primary_grey"
            app:layout_constraintBottom_toBottomOf="@+id/sugarAmount"
            app:layout_constraintEnd_toEndOf="@+id/minusbuttonmilk"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/minusbuttonmilk"
            app:layout_constraintTop_toTopOf="@+id/sugarAmount"
            app:layout_constraintVertical_bias="0.253" />

        <Button
            android:id="@+id/plusbuttonsugar"
            android:layout_width="61dp"
            android:layout_height="67dp"
            android:background="@drawable/rounded_outline"
            android:text="+"
            android:textSize="30sp"
            app:backgroundTint="@color/primary_grey"
            app:layout_constraintBottom_toBottomOf="@+id/minusbuttonsugar"
            app:layout_constraintEnd_toEndOf="@+id/plusbuttonmilk"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/plusbuttonmilk"
            app:layout_constraintTop_toTopOf="@+id/minusbuttonsugar"
            app:layout_constraintVertical_bias="0.253" />

        <View
            android:id="@+id/divider"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_marginBottom="150dp"
            android:background="?android:attr/listDivider"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/goButton3"
        android:layout_width="153dp"
        android:layout_height="91dp"
        android:background="@drawable/rounded_outline"
        android:text="@string/go"
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout8" />

</androidx.constraintlayout.widget.ConstraintLayout>